{
    "output": {
      "filePath": "repomix-isaaclab.txt",
      "style": "plain",
      "removeComments": false,
      "removeEmptyLines": false,
      "topFilesLength": 20,
      "showLineNumbers": false,
      "copyToClipboard": true
    },
    "include": [
        "scripts/reinforcement_learning/skrl/**",
        "source/isaaclab/isaaclab/envs/mdp/**",
        "source/isaaclab/isaaclab/managers/**",
        "source/isaaclab/isaaclab/scene/**",
        "source/isaaclab/isaaclab/sim/**",
        "source/isaaclab_tasks/isaaclab_tasks/manager_based/**"
    ],
    "ignore": {
      "useGitignore": true,
      "useDefaultPatterns": true,
      "customPatterns": [
        "**/spawners/**",
        "**/converters/**",
        "**/schemas/**",
        "**/allegro_hand/**",
        "**/anymal*/**",
        "**/ur_10/**",
        "**/go1/**",
        "**/go2/**",
        "**/g1/**",
        "**/h1/**",
        "**/spot/**",
        "**/classic/**",
        "**/locomotion/**",
        "**/lift/**",
        "**/stack/**",
        "**/cabinet/**",
        "**/inhand/**",
        "**/*.svg",
        "**/*.rst",
        "**/test/**",
        "uv.lock"
      ]
    },
    "security": {
      "enableSecurityCheck": true
    }
  }